{
	"New Test": {
		"prefix": "test",
		"body": [
			"#[${2|test,tokio::test|}]",
			"fn ${1:test_name}() {",
			"\t$0",
			"}"
		]
	},
	"New function": {
		"prefix": [
			"fn",
			"fun",
			"pub"
		],
		"body": [
			"${5|pub ,async ,pub async |}fn ${1:function_name}(${2:arguments}) ${3:-> ${4:ReturnType} }{",
			"\t$0",
			"}"
		]
	},
	"Add Derive": {
		"prefix": [
			"#[d",
			"der"
		],
		"body": [
			"#[derive(${0:Debug})]"
		]
	},
	"Format": {
		"prefix": [
			"for",
			"fmt"
		],
		"body": "format!($0)"
	},
	"Surround Format": {
		"body": "format!(\"${TM_SELECTED_TEXT/[\"]//g}\")"
	},
	"Implementation": {
		"prefix": "imp",
		"body": [
			"impl ${2:${3:Trait} for }${1:Struct} {",
			"\t$0",
			"}"
		]
	},
	"Attribute": {
		"prefix": "att",
		"body": "#[$0]"
	},
	"Struct": {
		"prefix": [
			"str",
			"pub"
		],
		"body": "struct ${1:Name}${2|{$3},($3)|}"
	}
}
